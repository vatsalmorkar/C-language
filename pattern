
#include <stdio.h>

int main() {

int i,j,k;

// for(a=0;a<=10;a++)
// {
    
//     printf("%d",a);
// }
for(i=1;i<=5;i++)
{
        for(k=1;k<=6-i;k++)
        {
            printf(" ");
        }
        for(j=6-i;j<=5;j++)
        {
    
                printf("%d ",j);
    
        }   
        
    printf("\n");
}
for(i=1;i<=5;i++)
{
     for(k=1;k<=i;k++)
        {
            printf(" ");
        }
        for(j=i;j<=5;j++)
        {
    
                printf("%d ",j);
    
        } 
       
        
    printf("\n");
}


    return 0;
}



